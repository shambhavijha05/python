   "source": [
    "# Iterating Through a string\n",
    "We can iterate through a string using a for loop."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "enter stringmount\n",
      "0 m\n",
      "1 o\n",
      "2 u\n",
      "3 n\n",
      "4 t\n"
     ]
    }
   ],
   "source": [
    "s=input(\"enter string\")\n",
    "for i in range(len(s)):\n",
    "    print(i,s[i])"
   ]
  },
