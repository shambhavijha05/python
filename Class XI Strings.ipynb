      "7\n"
     ]
    }
   ],
   "source": [
    "s=\"hello ,gh\"\n",
    "i=0\n",
    "c1=0\n",
    "while i<len(s):\n",
    "    if s[i].isalpha():\n",
    "        c1=c1+1\n",
    "    i+=1\n",
    "print(c1)"
   ]
  },
